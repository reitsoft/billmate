// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
      provider = "sqlite"
    url      = "file:./db.sqlite"
}

generator client {
  provider = "prisma-client-js"
}

model Kunden {
  id        Int    @id @default(autoincrement())
  nummer    String
  firma     String
  anrede    String
  vorname   String
  nachname  String
  mobil     String
  email     String
  adresse   String
  ort       String
  createdAt DateTime @default(now())
}

model Einheiten {
  id            Int    @id @default(autoincrement())
  bezeichnung   String
  abkuerzung    String
  createdAt     DateTime  @default(now())
}
