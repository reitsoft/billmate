// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}
model Kunden {
  id        String    @id @default(auto()) @map("_id") @db.ObjectId
  nummer    String
  firma     String
  anrede    String
  vorname   String
  nachname  String
  mobil     String
  email     String
  adresse   String
  ort       String
  createdAt DateTime @default(now())
}

model Einheiten {
  id            String    @id @default(auto()) @map("_id") @db.ObjectId
  bezeichnung   String
  abkuerzung    String
  createdAt     DateTime  @default(now())
}
